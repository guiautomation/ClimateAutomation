		<clicklink locatortype='linkText'>Get Online Quote</clicklink>
		<waitseconds>4</waitseconds>
		
		<typetext locatortype='id' locator='address'>    Newton County, MO
    WB ID: 368194374</typetext>
        <clickbutton locatortype='id'>addressSubmit</clickbutton>
        
